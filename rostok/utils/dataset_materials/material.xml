<?xml version="1.0" encoding="UTF-8"?>
<material>

    <rubber>
        <ChMaterialSurfaceNSC>
            <Friction>0.7</Friction>
            <DampingF>0.1</DampingF>
        </ChMaterialSurfaceNSC>
    </rubber>

    <polyactide>
        <ChMaterialSurfaceNSC>    
            <Friction>0.4</Friction>
            <DampingF>0.1</DampingF>
        </ChMaterialSurfaceNSC>        
