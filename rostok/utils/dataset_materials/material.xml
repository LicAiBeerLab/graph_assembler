    <steel>
        <ChMaterialSurfaceSMC>
            <YoungModulus>210</YoungModulus>
            <PoissonRatio>0.28</PoissonRatio>
            <Kt>100</Kt>
            <Kn>100</Kn>
            <Gt>10</Gt>
            <Gn>10</Gn>
        </ChMaterialSurfaceSMC>
    </steel>
</material>